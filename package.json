{
  "name": "npm-ts-template",
  "version": "1.0.2",
  "description": "npm发包模板",
  "author": "yaoxfly",
  "license": "MIT",
  "private": false,
  "module": "./lib/esm/index.js",
  "main": "./lib/cjs/index.js",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.j"
  },
  "types": "typings/index.d.ts",
  "scripts": {
    "rollup": "node ./node_modules/rollup/dist/bin/rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npm version patch -f",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix"
  },
  "files": [],
  "bin": {},
  "engines": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.cjs"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": [
      "npm run lint"
    ]
  },
  "keywords": [
    "npm",
    "发包",
    "模板",
    "template",
    "rollup",
    "husky",
    "lint-staged",
    "commitlint",
    "commitizen"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaoxfly/npm-ts-template.git"
  },
  "bugs": {
    "url": "https://github.com/yaoxfly/npm-ts-template/issues"
  },
  "homepage": "https://github.com/yaoxfly/npm-ts-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "rollup": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-externals": "^5.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.3"
  }
}
